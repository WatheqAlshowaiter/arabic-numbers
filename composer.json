{
  "name": "4myth/arabic-numbers",
  "type": "library",
  "description": "Laravel Middleware convert arabic numbers to english numbers",
  "keywords": [
    "Myth",
    "4myth",
    "arabic",
    "indian",
    "arabic",
    "macos",
    "mac"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Ahmed Fayez",
      "email": "mythpe@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "laravel/framework": ">=5.5"
  },
  "require-dev": {},
  "autoload": {
    "psr-4": {
      "Myth\\Support\\ArabicNumbers\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Myth\\Support\\ArabicNumbers\\Providers\\ArabicNumbersProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
